{
  "name": "pom-cypress-e2e",
  "version": "1.0.0",
  "description": "Applying Page Object Model with Cypress to End to End Testing",
  "main": "index.js",
  "scripts": {
    "test": "cypress:open",
    "test:login": "npx cypress run --spec cypress/e2e/shop-tests/login.cy.js",
    "test:register": "npx cypress run --spec cypress/e2e/shop-tests/register.cy.js",
    "test:addToCart": "npx cypress run --spec cypress/e2e/shop-tests/addToCart.cy.js"
  },
  "keywords": [
    "cypress",
    "test",
    "pom",
    "e2e"
  ],
  "author": "Omar Centeno <ODCenteno>",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.1.0",
    "mochawesome": "^7.1.3"
  },
  "dependencies": {
    "faker": "^5.5.3"
  }
}
